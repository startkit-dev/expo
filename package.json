{
  "name": "startkit-expo",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "check": "concurrently bun:type-check bun:lint bun:format:check bun:test",
    "clean": "git clean -xdf .expo",
    "dev": "expo start",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "format:check": "npx prettier --check . --ignore-path .gitignore",
    "ios": "expo start --ios",
    "lint": "eslint .",
    "outdated": "npx npm-check-updates --interactive --format group",
    "start": "expo start",
    "test": "jest --watchAll",
    "type-check": "tsc --noEmit --pretty",
    "web": "expo start --web"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  },
  "dependencies": {
    "@expo-google-fonts/space-mono": "^0.2.3",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/native": "^6.1.9",
    "expo": "~49.0.21",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.14",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-web-browser": "~12.3.2",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.9"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@types/jest": "^29.5.10",
    "@types/react": "~18.2.41",
    "@types/react-test-renderer": "^18.0.7",
    "@vercel/style-guide": "^5.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "jest": "^29.7.0",
    "jest-expo": "~49.0.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  }
}
